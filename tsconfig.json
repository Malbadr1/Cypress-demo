{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "types": ["cypress", "node"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["cypress/pages/*"]
    }
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"]
}
